{
  "name": "vscode-open-in-typora",
  "displayName": "Open in Typora",
  "description": "open Markdown files from VSCode in Typora",
  "version": "1.0.2",
  "publisher": "cyberbiont",
  "repository": {
    "type": "git",
    "url": "https://github.com/cyberbiont/vscode-open-in-typora.git"
  },
  "license": "MIT",
  "icon": "images/app-icon-typora.png",
  "engines": {
    "vscode": "^1.38.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "Markdown",
    "Typora",
    "open",
    "external editor"
  ],
  "activationEvents": [
    "onCommand:typora.open",
    "onLanguage:markdown"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "typora.open",
        "title": "Open in Typora",
        "category": "Typora",
        "when": "editorLangId == markdown"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "typora.open",
          "group": "navigation",
          "when": "editorLangId == markdown"
        }
      ],
      "explorer/context": [
        {
          "command": "typora.open",
          "group": "navigation",
          "when": "resourceLangId == markdown"
        }
      ]
    }
  },
  "scripts": {
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "eslint": "^6.8.0"
  },
  "dependencies": {}
}
